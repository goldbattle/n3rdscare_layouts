{
  "name": "n3rdscare_layouts",
  "version": "1.0.0",
  "description": "Simple overlay for charity streams using the N3RDS.CARE website.",
  "author": {
    "name": "goldbattle",
    "url": "https://github.com/goldbattle"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "keywords": [
    "NodeCG",
    "Extra Life",
    "nodecg-bundle"
  ],
  "nodecg": {
