      {
        "file": "disp_goal.html",
        "width": 265,
        "height": 65,
        "singleInstance": true
      },
      {
        "file": "disp_donation.html",
        "width": 665,
        "height": 65,
        "singleInstance": true
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "Apache-2.0",
  "dependencies": {
    "aja": "^0.4.1",
    "numeral": "^2.0.6",
    "profanity-filter": "^0.2.1",
    "q": "^1.5.1",
    "request": "^2.88.0",
    "request-promise": "^4.0.2"
  }
}
